{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "nodeVersion": "16.x"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/file/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }